:OPC a owl:Class ;
    rdfs:subClassOf :Node .

:OriginalEndNode a owl:ObjectProperty ;
    rdfs:domain :Edge ;
    rdfs:range :Node .

:OriginalFlowDir a owl:DatatypeProperty ;
